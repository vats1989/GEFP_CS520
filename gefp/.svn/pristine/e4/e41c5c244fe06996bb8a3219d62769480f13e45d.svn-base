package gefp.model;

import java.io.Serializable;
import java.util.List;

import javax.persistence.*;

@Entity
public class Department implements Serializable{

	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy= GenerationType.AUTO)
	@Column(name="dept_id")
	private long id;
	
	@Basic
	@Column(name="", nullable=false)
	private String dept_name;

	@ManyToMany(mappedBy="departments",cascade={CascadeType.PERSIST,CascadeType.MERGE},targetEntity= Stage.class)
	private List<Stage> Stages;

	@ManyToMany(mappedBy="departments",cascade={CascadeType.PERSIST,CascadeType.MERGE},targetEntity= Runway.class)
	private List<Runway> runways;

	@OneToMany(mappedBy="department",cascade={CascadeType.PERSIST,CascadeType.MERGE},targetEntity= CheckPoint.class)
	private List<CheckPoint> checkpoints;
			
	@OneToMany(mappedBy="department",cascade={CascadeType.PERSIST,CascadeType.MERGE},targetEntity= User.class)
	private List<User> Users;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getDept_name() {
		return dept_name;
	}

	public void setDept_name(String dept_name) {
		this.dept_name = dept_name;
	}

	public List<Stage> getStages() {
		return Stages;
	}

	public void setStages(List<Stage> stages) {
		Stages = stages;
	}

	public List<Runway> getRunways() {
		return runways;
	}

	public void setRunways(List<Runway> runways) {
		this.runways = runways;
	}

	public List<CheckPoint> getCheckpoints() {
		return checkpoints;
	}

	public void setCheckpoints(List<CheckPoint> checkpoints) {
		this.checkpoints = checkpoints;
	}

	public List<User> getUsers() {
		return Users;
	}

	public void setUsers(List<User> users) {
		Users = users;
	}
}